{
  "name": "weather-dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && npm run server:start",
    "start:dev": "concurrently \"npm run server:dev\" \"wait-on tcp:3000 && npm run client:dev\"",
    "server:start": "node dist/server.js",
    "server:dev": "nodemon --watch server/src --ext ts --exec \"node --loader ts-node/esm\" server/src/server.ts",
    "install": "npm install --legacy-peer-deps && cd client && npm install",
    "client:build": "cd client && npm run build",
    "client:dev": "cd client && vite --port 3000",
    "build": "npm run client:build && tsc",
    "render-build": "npm install && cd client && npm install && cd .. && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.4.2",
    "@types/node-fetch": "^3.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "uuid": "^10.0.0"
  }
}
