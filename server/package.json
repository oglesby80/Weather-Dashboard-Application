{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for Weather Dashboard",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cd client && npm run build",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm\" src/server.ts",
    "client:build": "cd client && npm run build"
  },
  "dependencies": {
    "express": "^4.19.2",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/express": "^4.17.21",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.5",
    "ts-node": "^10.9.1"
  }
}







