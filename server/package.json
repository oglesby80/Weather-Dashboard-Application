{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for Weather Dashboard",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm src/server.ts\""
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,json,js",
    "exec": "npx tsc && node dist/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "nodemon": "^3.1.0",
    "@types/node": "^20.4.2",
    "@types/express": "^4.17.21",
    "ts-node": "^10.9.1"
  }
}






